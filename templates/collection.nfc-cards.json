    "b04302c2-df15-4d73-9748-2d62ebe84660": {
      "type": "layered-images-with-text",
      "blocks": {
        "template--20294491930924__b04302c2-df15-4d73-9748-2d62ebe84660-heading-1": {
          "type": "heading",
