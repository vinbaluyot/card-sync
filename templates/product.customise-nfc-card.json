    "aa571a86-746e-4b78-8461-2bc599f367dc": {
      "type": "text-with-icons",
      "blocks": {
        "template--20911728263468__aa571a86-746e-4b78-8461-2bc599f367dc-icon-1": {
          "type": "icon",
          "settings": {
            "icon": "ruler",
            "heading": "Dimensions",
            "text": "<p>Our cards can be printed with a full to 2mm bleed design and have dimensions of 54mm x 86mm<\/p>"
          }
        },
        "template--20911728263468__aa571a86-746e-4b78-8461-2bc599f367dc-icon-2": {
          "type": "icon",
          "settings": {
            "icon": "ruler",
            "image": "shopify:\/\/shop_images\/icons8-file-format-32.png",
            "heading": "File Format",
            "text": "<p>We accept PNG, JPEG, SVG & PDF. Vector Images work best with engraving<\/p>"
          }
        },
        "template--20911728263468__aa571a86-746e-4b78-8461-2bc599f367dc-icon-3": {
          "type": "icon",
          "settings": {
            "icon": "ruler",
            "image": "shopify:\/\/shop_images\/icons8-transparent-96.png",
            "heading": "Image",
            "text": "<p>We recommend uploading images with transparent or black\/white backgrounds for best results. Uploads up to 10mb if exceeds you can send directly to our email.<\/p>"
          }
        },
        "05200b8c-dce1-4009-bd2a-83cb704170ca": {
          "type": "icon",
          "settings": {
            "icon": "ruler",
            "image": "shopify:\/\/shop_images\/icons8-resolution-32.png",
            "heading": "Resolution",
            "text": "<p>Provide files with a high resolution to ensure clarity and sharpness. Avoid low-resolution or pixelated images<\/p>"
          }
        }
      },
