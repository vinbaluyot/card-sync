    "e199fb67-f255-402b-9092-896c6051627a": {
      "type": "layered-images-with-text",
      "blocks": {
        "template--20073777627436__e199fb67-f255-402b-9092-896c6051627a-1689731481b23da608-0": {
          "type": "subheading",
          "disabled": true,
          "settings": {
            "text": "Add a tagline"
