          }
        },
        "template--19290746028332__d3ec07c3-360f-4389-ae64-721a28b8e59d-16853224406e9a1780-2": {
          "type": "column",
          "settings": {
            "title": "Customise",
            "heading_size": "h4",
            "text": "<p>To personalise your NFC business cards, upload your logo design or any other text you want to include on the cards. Select the integrations you would like and provide details and we will configure and create your landing page.<\/p>",
            "link_label": "",
            "link": ""
          }
        },
