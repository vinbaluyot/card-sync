    "a8052c75-b6b7-416b-9a4a-9429cc92b96e": {
      "type": "layered-images-with-text",
      "blocks": {
        "template--20333461176620__a8052c75-b6b7-416b-9a4a-9429cc92b96e-subheading-1": {
          "type": "subheading",
          "settings": {
            "text": "NEXT GENERATION NETWORKING"
