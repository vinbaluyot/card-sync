    "e47700cd-a97e-49fe-9e2a-e27b95f5f411": {
      "type": "image-with-text",
      "blocks": {
        "template--21590001647916__e47700cd-a97e-49fe-9e2a-e27b95f5f411-subheading-1": {
          "type": "subheading",
          "disabled": true,
          "settings": {
            "text": "Add a tagline"
